<div id='about'>
    <%let a=data.About.Why;%>
    <%let b=data.About.Why2;%>
    <%let para=data.About.paragraph;%>
    <div class='container'>
      <div class='row'>
        <div class='col-xs-12 col-md-6'>
          <img src='img/about.jpg' class='img-responsive' alt='' />{' '}
        </div>
        <div class='col-xs-12 col-md-6'>
          <div class='about-text'>
            <h2>About Us</h2>
            <p><%=para%>></p>
            <h3>Why Choose Us?</h3>
            <div class='list-style'>
              <div class='col-lg-6 col-sm-6 col-xs-12'>
                <ul>
                    <%a.map(function(element,index){%>
                        <li><%=element%></li>
                    <%})%>
                </ul>
              </div>
              <div class='col-lg-6 col-sm-6 col-xs-12'>
                <ul>
                    <%b.map(function(element,index){%>
                        <li><%=element%></li>
                    <%})%>
                </ul>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

