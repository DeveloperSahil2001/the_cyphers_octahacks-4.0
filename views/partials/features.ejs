<div id="features" class="text-center">
  <div class="container">
    <div class="col-md-10 col-md-offset-1 section-title">
      <h2>Features</h2>
    </div>
    <div class="row">
      <% let features=data.Features;%>
       <%features.map(function(element,index){%>
      <div class="col-xs-6 col-md-3">
        <i class="<%=element.icon%>"></i>
        <h3><%=element.title%></h3>
        <p><%=element.text%></p>
      </div>
      <%})%>
    </div>
  </div>
</div>
